{
  "name": "zero-sum-nutrition",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter @zsn/web dev",
    "dev:worker": "pnpm --filter @zsn/queue-processor dev",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.1",
    "playwright": "^1.58.1",
    "prettier": "^3.4.2",
    "turbo": "^2.8.1",
    "typescript-eslint": "^8.19.1"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "sharp",
      "msgpackr-extract"
    ]
  }
}
